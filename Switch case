#include<stdio.h>
int main()
{
printf("Pick an item:\n1.Pasta,Rs.179\n2.Sandwich,Rs.149\n3.French Fries,Rs.99\n4.Burger,Rs.129\n5.Pizza,Rs.249");
int choice;
scanf("%d",&choice);
switch(choice)
{
case 1:
printf("Food item - Pasta\n Price - Rs.179");
break;
case 2 :
printf("Food item - Sandwich\n Price - Rs.149");
break;
case 3:
printf("Food item - French Fries\n Price - Rs.99");
break;
case 4:
printf("Food item - Burger\n Price - Rs.129");
break;
case 5:
printf("Food item - Pizza\n Price - Rs.249");
break;
default:
printf("Invalid choice");
}
    return 0;
}
